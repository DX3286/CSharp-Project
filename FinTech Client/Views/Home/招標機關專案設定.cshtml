    <script>
        $(document).ready(function () {
            $.get("@Url.Action("getUser", "Home")", null, function (data) {
                user = data;
                //alert(user);
            });

            $("input[name=importantString]").change(function () {
                var e = $(this).val();
                e = e.replace(/\s+/g, '');
                $(this).val(e);
            });
        });

        function myClick() {
            var c = $("#caseid").val();
            var r = $("#rec").val();
            var w = $("#who").val();

            if (c == "" || r == "" || w == "") {
                alert("不能空白");
                return;
            }

            $.post("@Url.Action("AddNewCase", "Fintech")", { caseid: c, receipt: r, who: w, whoami: user }, function (data) {
                if (data == 1) {
                    alert("成功新增");
                    location.href = '@Url.Action("招標機關功能", "Home")';
                }
                if (data == 0) {
                    alert("無法新增，請確認輸入資訊是否有誤");
                }
                if (data == -1) {
                    alert("案號不存在");
                }
            });
        }
