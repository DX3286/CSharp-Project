    <script>
        function myclick() {
            var a = $("#myBank").val();
            var b = $("#myAccName").val();
            var c = $("#myAccId").val();
            $.get("@Url.Action("getUser", "Home")", null, function (data) {
                var user = data;
                //alert(user);
                //alert(a);
                //alert(b);
                //alert(c);

                if (a == "" || b == "" || c == "") {
                    alert("不能空白");
                    return;
                }

                $.post("@Url.Action("AddBankAcc", "Fintech")", { userid: user, bankname: a, accname: b, accid: c }, function (data) {
                    switch (data) {
                        case 0:
                            alert("該帳號已存在")
                            break;
                        case 1:
                            alert("帳戶名稱重複")
                            break;
                        case 2:
                            alert("帳戶已達上限3個，由帳戶設定管理")
                            break;
                        case 3:
                            alert("新增成功")
                            location.href = "@Url.Action("投標廠商功能", "Home")"
                            break;
                    }
                })
            });
        }
