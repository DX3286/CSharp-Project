    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script>
        var user;

        var perfEntries = performance.getEntriesByType("navigation");

        if (perfEntries[0].type === "back_forward") {
            location.reload(true);
        }

        $(document).ready(function () {
            $.get("@Url.Action("getUser", "Home")", null, function (data) {
                user = data;
                //alert(user);
                $.post("@Url.Action("AccountManagement", "Fintech")", { id: user }, function (data) {
                    if (data[0] == "Empty") {
                        $('#acc1').val("");
                        $('#amount1').prop('disabled', true);
                    } else {
                        $('#acc1').val(data[0]);
                        $('#amount1').prop('disabled', false);
                    }

                    if (data[1] == "Empty") {
                        $('#acc2').val("");
                        $('#amount2').prop('disabled', true);
                    } else {
                        $('#acc2').val(data[1]);
                        $('#amount2').prop('disabled', false);
                    }

                    if (data[2] == "Empty") {
                        $('#acc3').val("");
                        $('#amount3').prop('disabled', true);
                    } else {
                        $('#acc3').val(data[2]);
                        $('#amount3').prop('disabled', false);
                    }

                    updateRemain(-1);
                });
            });

            //$("#testlist").change(function () {
            //    //alert($("#testlist").val());

            //   updateRemain();
            //});
        });

        var delaytimer;

        function updateRemain(i) {

            // Update all 3 in one time
            if (i == -1) {

                $.post("@Url.Action("ShowRemainAll", "Fintech")", { id: user }, function (data) {
                    $("#remain1").val(data[0]);
                    $("#remain2").val(data[1]);
                    $("#remain3").val(data[2]);
                })
                return;
            }

            $("#confirmBtn").prop("disabled", true);
            clearTimeout(delaytimer);
            delaytimer = setTimeout(function () {

                // Normal update
                var a = $("#acc" + i).val();
                var b = $("#amount" + i).val();

                if (a == "") {
                    return;
                }

                if (b < 0) {
                    b = 0;
                    $("#amount" + i).val(0);
                }

                $.post("@Url.Action("ShowRemain", "Fintech")", { id: a }, function (data) {
                    //alert("!!" + data);
                    $("#remain" + i).val(data - b);
                });

                var c = Number( $("#amount1").val() ) + Number( $("#amount2").val() ) + Number( $("#amount3").val());
                $("#totalAmount").val(c);

                $("#confirmBtn").prop("disabled", false);
            }, 500);
        }

        function myclick() {
            var acc1 = $("#acc1").val();
            var acc2 = $("#acc2").val();
            var acc3 = $("#acc3").val();

            var amo1 = $("#amount1").val();
            var amo2 = $("#amount2").val();
            var amo3 = $("#amount3").val();

            var rm1 = $("#remain1").val();
            var rm2 = $("#remain2").val();
            var rm3 = $("#remain3").val();

            if (amo1 == "0" && amo2 == "0" && amo3 == "0") {
                alert("請設定金額");
                return;
            }
            if (Number(rm1) < 0 || Number(rm2) < 0 || Number(rm3) < 0) {
                alert("餘額不足");
                return;
            }

            // 產生憑證
            var total = $("#totalAmount").val();
            var a1s = "0";
            var a2s = "0";
            var a3s = "0";

            if (amo1 != "0") {
                a1s = (acc1 + "=" + amo1);
            }

            if (amo2 != "0") {
                a2s = (acc2 + "=" + amo2);
            }

            if (amo3 != "0") {
                a3s = (acc3 + "=" + amo3);
            }

            $.post("@Url.Action("GenReceipt", "Fintech")", { uid: user, a1: a1s, a2: a2s, a3: a3s, amount: total }, function (data) {
                //alert(data);
                if (data != "Error") {
                    var url = "@Url.Action("憑證產出", "Home", new { receipt = "dat" })";
                    url = url.replace("dat", data);
                    location.href = url;
                } else {
                    alert("Error");
                }
            });

            /**
            var url = "Html.Raw(Url.Action("憑證產出", "Home", new { a1 = "-1r", a2 = "-2r", a3 = "-3r" }))";
            location.href = url;
            */
        }
    </script>

